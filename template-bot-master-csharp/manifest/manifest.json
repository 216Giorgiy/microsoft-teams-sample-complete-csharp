{
  "$schema": "https://statics.teams.microsoft.com/sdk/v1.0/manifest/MicrosoftTeams.schema.json",
  "manifestVersion": "1.0",
  "version": "1.0",
  "id": "<<REGISTERED_BOT_ID>>",
  "packageName": "com.microsoft.teams.tutorial.csharp",
  "developer": {
    "name": "Microsoft",
    "websiteUrl": "https://www.microsoft.com",
    "privacyUrl": "https://www.microsoft.com/privacy",
    "termsOfUseUrl": "https://www.microsoft.com/termsofuse"
  },
  "name": {
    "short": "Teams Tutorial App",
    "full": "Teams Tutorial App"
  },
  "description": {
    "short": "This is an app made from the Teams App tutorial.",
    "full": "This is an app made from the Teams App tutorial - as you progress, more functionality will be added."
  },
  "icons": {
    "outline": "bot_blue.png",
    "color": "bot_blue.png"
  },
  "accentColor": "#0078D7",
  "configurableTabs": [
    {
      "configurationUrl": "<<BASE_URI>>/tab/config/config.html",
      "canUpdateConfiguration": false,
      "scopes": [
        "team"
      ]
    }
  ],
  "staticTabs": [
    {
      "contentUrl": "<<BASE_URI>>/tab/content/content.html",
      "entityId": "simpleTabContent",
      "name": "Simple Tab",
      "scopes": [
        "personal"
      ]
    }
  ],
  "bots": [
    {
      "botId": "<<REGISTERED_BOT_ID>>",
      "scopes": [
        "team",
        "personal"
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "<<BASE_URI_DOMAIN>>"
  ]
}